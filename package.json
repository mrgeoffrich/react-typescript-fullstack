{
    "name": "react-typescript-fullstack",
    "version": "1.0.0",
    "description": "Starter kit for a typescript full stack solution.",
    "main": "build/server/server.js",
    "scripts": {
        "prod:build:client": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config ./src/webpack/webpack.config.prod.js",
        "prod:build:server": "cross-env NODE_ENV=production tsc -p ./src/server",
        "prod:build": "rimraf build && yarn run prod:build:server && yarn run prod:build:client",
        "prod:run": "cross-env NODE_ENV=production node build/server/server.js",
        "test": "tsc && mocha build/**/*.spec.js",
        "dev": "rimraf build && cross-env NODE_ENV=development tsc-watch -p ./src/server --onSuccess \"node ./build/server/server.js\"",
        "clean": "rimraf build && rimraf .awcache && rimraf .localStorage"
    },
    "keywords": [
        "vmware",
        "node",
        "react",
        "CI"
    ],
    "author": "Geoff Rich",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/mrgeoffrich/homelab-vm-builder"
    },
