        "fork-ts-checker-notifier-webpack-plugin": "^2.0.0",
        "fork-ts-checker-webpack-plugin": "^4.1.6",
        "glob": "^7.1.2",
        "left-pad": "^1.3.0",
        "load-json-file": "^6.2.0",
        "morgan": "^1.9.0",
        "react": "^16.3.2",
        "react-dom": "^16.3.2",
        "react-helmet": "^6.0.0",
        "react-hot-loader": "^4.1.2",
        "react-redux": "^7.2.0",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
